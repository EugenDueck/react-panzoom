{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/jsx-filename-extension": 0
  },
  "globals": {
    "console": "false",
    "expect": "false",
    "test": "false",
    "window": "false"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    },
    "react": {
      "version": "16.9.0"
    }
  },
  "parser": "babel-eslint"
}